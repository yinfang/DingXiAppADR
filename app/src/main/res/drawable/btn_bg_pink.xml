<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="20dp"/>
            <stroke android:width="1dp" android:color="@color/btn_press_color"/>
            <solid android:color="@color/btn_press_color"/>
        </shape>
    </item>

    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <corners android:radius="20dp"/>
            <stroke android:width="1dp" android:color="@color/btn_normal_color"/>
            <solid android:color="@color/btn_normal_color"/>
        </shape>
    </item>
</selector>